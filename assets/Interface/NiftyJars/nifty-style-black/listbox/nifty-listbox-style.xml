<?xml version="1.0" encoding="UTF-8"?><nifty-styles>  <style id="nifty-listbox">    <attributes />  </style>  <style id="nifty-listbox#scrollpanel">    <attributes focusable="true" padding="1px"/>    <effect overlay="true">      <onActive name="colorBar" color="#0000" post="false" neverStopRendering="true" timeType="infinite" />      <onActive name="border" border="1px" color="#eeaa06" inset="0px,0px,0px,1px"/>      <onEnabled name="renderQuad" startColor="#2228" endColor="#2220" post="false" length="150" />      <onDisabled name="renderQuad" startColor="#2220" endColor="#2228" post="false" length="150" />    </effect>  </style>  <style id="nifty-listbox#bottom-right">    <attributes width="23px"  />  </style>  <style id="nifty-listbox-item" base="nifty-label">    <attributes color="#000f" width="100%" align="left" textVAlign="center" textHAlign="left" />    <interact onClick="listBoxItemClicked()" />  </style></nifty-styles>